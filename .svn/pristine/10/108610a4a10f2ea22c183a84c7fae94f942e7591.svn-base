<?xml version="1.0" encoding="UTF-8"?>
<sld:StyledLayerDescriptor 
    xmlns:sld="http://www.opengis.net/sld" 
    xmlns:gml="http://www.opengis.net/gml"
    xmlns="http://www.opengis.net/sld" 
    xmlns:ogc="http://www.opengis.net/ogc" 
    version="1.0.0">
    <sld:NamedLayer>
        <sld:Name>kais_sig_as</sld:Name>
        <sld:UserStyle>
            <sld:Name>kais_sig_as</sld:Name>
            <sld:Title>kais_sig_as</sld:Title>
            <sld:IsDefault>1</sld:IsDefault>
            <sld:FeatureTypeStyle>
                <sld:Name>kais_sig_as</sld:Name>
                <sld:Rule>
                    <sld:Name>basicPolyStyle</sld:Name>
                    <sld:PolygonSymbolizer>
                        <sld:Geometry>
                            <ogc:PropertyName>_geometry</ogc:PropertyName>
                        </sld:Geometry>
                        <sld:Fill>
                            <sld:CssParameter name="fill">#eaebe9</sld:CssParameter>
                            <sld:CssParameter name="fill-opacity">0.337</sld:CssParameter>
                        </sld:Fill>
                        <sld:Stroke>
                            <sld:CssParameter name="stroke">#009933</sld:CssParameter>
                            <sld:CssParameter name="stroke-opacity">0.337</sld:CssParameter>
                            <sld:CssParameter name="stroke-width">4</sld:CssParameter>
                        </sld:Stroke>
                    </sld:PolygonSymbolizer>
                </sld:Rule>
            </sld:FeatureTypeStyle>
        </sld:UserStyle>
    </sld:NamedLayer>
</sld:StyledLayerDescriptor>