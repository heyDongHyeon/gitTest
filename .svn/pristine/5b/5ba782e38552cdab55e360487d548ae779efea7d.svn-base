@CHARSET "UTF-8";

html, body { background: #EDEDED; margin: 0; padding: 0; }
img { padding: 0; margin: 0; }
.hidden { display: none; }
input, select { outline: none; }

#header {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
    height: 50px;
    background: #3A79E2;
    border-bottom: 2px solid #255DBA;
}

#header #ci {
    margin-left: 18.5%;
}

#header #back {
    position: absolute;
    top: 0px;
    right: 18.5%;
    background: gray;
    color: white;
    padding: 15px;
    cursor: pointer;
}

#header #back:hover {
    text-decoration: underline;
}

