#roadview_pop {
    margin: 15px 0;
    position: absolute;
    z-index: 9999;
    right: 0;
    width: 500px;
    bottom: 0;
}
#roadview_pop #roadview_title_bar {
    height: 30px;
    color: #f6f6f6;
    background: #3e3f48;
}
#roadview_pop #roadview_title {
    float: left;
    font-size: 14px;
    font-weight: 800;
    margin-top: 8px;
    margin-left: 8px;
}
#roadview_pop #roadview_close {
    float: right;
    background: transparent;
    border: none;
    margin: 3px 0;
}
#roadview_pop #roadview_close img{width: 20px;}
#roadview_pop #roadview {border: 1px solid black;height: 350px;}





