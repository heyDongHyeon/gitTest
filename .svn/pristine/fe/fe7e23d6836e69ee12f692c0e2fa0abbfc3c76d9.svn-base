@CHARSET "UTF-8";

/* 검색 */
#search-parent {
    position: absolute;
    width: 100%;
    height: 33px;
    z-index: 1;
    list-style: none;
    top: 0px;
    left: 136px;
    background: #3e3f48;
    color: white;
    display: none;
    padding-left: 5px;
}
#searchWrap {
    text-align: left;
    margin: 1px;
}
#searchWrap td {
    line-height: 26px;
    padding: 0px 5px;
    color: white;
    font-size: 12px;
}
#searchWrap td input[type=checkbox] {
    vertical-align: middle;
}
#searchWrap select {
    background: #30303A;
    color: white;
    border: none;
    font-size: 12px;
    padding: 3px 0px;
}
#searchWrap select::-ms-expand {
    display: none;
}
#searchWrap input[type=text] {
    background: #30303A;
    color: white;
    border: none;
    padding: 3px 0px;
    padding-left: 5px;
}
/* 171212 추가 */
#searchWrap #jibunSearch {
    position: absolute;
    top: 0px;
    height: 30px;
}
#searchHead {
    position: relative;
    border-top: 1px solid #30303a;
    background: #3E3F48;
    margin-top: 3px;
}
#searchMode {
    float: left;
    margin: 4px;
}
#searchToggle {
    position: absolute;
    right: 15px;
    top: 15px;
}

/* 스케일 */
#scale-wrap {
    position: absolute;
    width: 89px;
    z-index: 1;
    list-style: none;
    top: 0px;
    left: 41px;
    padding: 7px;
    background: #3e3f48;
    color: white;
    display: none;
    padding-left: 15px;
    border-right: 1px solid #30303A;
}

#scale-val {
    width: 63px;
    height: 18px;
    outline: none;
    background: #30303A;
    color: white;
    padding: 2px 0px 2px 5px;
}
.searchBtn {
	padding: 3px 15px;
}
