/* 관심영역 */
#fav-wrap {
    display: none;
    position: absolute;
    left: 50px;
    top: 325px;
    width: 300px;
    height: 250px;
    background: white;
    border: 1px solid #D3D3D3;
    border-radius: 0px 0px 10px 10px;
}

#fav-wrap #fav-header {
    width: 100%;
    height: 30px;
    background: #3E3F48;
}

#fav-wrap #fav-body {
    width: 280px;
    height: 200px;
    padding: 10px;
}

#fav-wrap #fav-body #fav-name {
    width: 188px;
    height: 26px;
    border: 1px solid #E8E8E8;
    font-size: 14px;
    color: #3a4452;
    font-weight: bold;
    padding-left: 10px;
    border-radius: 5px 0px 0px 5px;
}

#fav-wrap #fav-body #fav-add {
    width: 78px;
    height: 30px;
    border: 1px solid #E8E8E8;
    font-size: 14px;
    color: #3a4452;
    font-weight: bold;
    border-radius: 0px 5px 5px 0px;
    background: #EFEFEF;
    cursor: pointer;
}

#fav-wrap #fav-content {
    width: 100%;
    height: 160px;
    overflow: auto;
    margin-top: 10px;
}

#fav-wrap #fav-content .fav-list {
    padding: 7px;
    color: black;
    font-size: 14px;
    font-weight: bold;
    border-bottom: 2px solid #B4B4B4;
}

#fav-wrap #fav-content .fav-list:hover {
    background: #f1f1f1;
}

#fav-wrap #fav-content .fav-list span {
    width: 224px;
    cursor: pointer;
    display: inline-block;
}

#fav-wrap #fav-content .fav-list .fav-del {
    font-size: 14px;
    font-weight: normal;
    padding: 0px;
    background: none;
    border: none;
    /* border-bottom: 1px solid gray;
    position: absolute;
    right: 35px; */
    outline: none;
    cursor: pointer;
/*     text-decoration: underline; */
}

#fav-wrap #fav-content .fav-list .fav-del:hover {
    font-weight: bold;
}

#fav-wrap #fav-title {
    position: absolute;
    font-weight: bold;
    font-size: 14px;
    color: white;
    left: 10px;
    top: 7px;
}

#fav-wrap #fav-header #popup-closer {
    position: absolute;
    right: 0;
    cursor: pointer;
}