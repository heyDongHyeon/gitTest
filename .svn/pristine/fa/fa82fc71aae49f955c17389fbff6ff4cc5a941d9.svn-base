.stat_btn {
    height: 34px;
    background: #4582ac;
    width: 100px;
    border: none;
    color: #f6f6f6;
    margin: 0;
    cursor: pointer;
    margin-left: 7px;
}

.stat_gray_btn {
    background: #555;
}

.stat_gray_btn:hover {
    background: #4582ac;
}

.stat_event {
    width : 190px !important;
}

#stat_change_btn, #stat_excel_btn {
	float: right;
}

#search select.non{
	display: none;
}

#search span.non{
	display: none;
}

#search select option.non{
	display: none;
}

#search select option.active{
	display: block !important;
}

#stat_wrap{
	background : rgb(59, 59, 59)
}

#stat_wrap #wrap{
	height:100% !important;
}

#stat_wrap #wrap > div{
	color: #f6f6f6;
}

