#wrap #content #stat_table_title{
	font-weight: bold;
    font-size: 14px;
    margin:10px 0;
    padding-left: 10px;
    background: rgb(59, 59, 59);
}

#wrap #content #stat_table_type{
	display: none;
}

#wrap #content #stat_chart_type{
	background:none;
}
